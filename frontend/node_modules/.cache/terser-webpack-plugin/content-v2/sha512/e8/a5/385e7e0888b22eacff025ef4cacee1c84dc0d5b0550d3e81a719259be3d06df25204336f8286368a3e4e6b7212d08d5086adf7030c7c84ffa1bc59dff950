;\n      history.push(\"/admin/categorias/\");\n    } else {\n      if (!categorie?.name || categorie._id !== Number(categorieId))\n        dispatch(detailsCategorie(categorieId));\n      else setName(categorie.name);\n    }\n  }, [history, userInfo, success, categorie, categorieId, dispatch]);\n\n  return (\n    <div style={{ marginTop: \"10vh\" }}>\n      <main>\n        {loadingCategorie ? (\n          <Loader />\n        ) : errorCategorie ? (\n          <Message variant=\"danger\">{errorCategorie}</Message>\n        ) : (\n          <FormContainer>\n            <h2>Editar categora</h2>\n            <Form onSubmit={submitHandler}>\n              <Form.Group controlId=\"name\">\n                <Form.Label>Nombre de categora</Form.Label>\n                <Form.Control\n                  type=\"text\"\n                  placeholder=\"Ingresa el nombre de la categora\"\n                  value={name}\n                  onChange={(e) => setName(e.target.value)}\n                  required\n                ></Form.Control>\n              </Form.Group>\n\n              {loading ? (\n                <Loader />\n              ) : error ? (\n                <>\n                  <Message variant=\"danger\">{error}</Message>\n                  <Button type=\"submit\" variant=\"primary\">\n                    Actualizar categora\n                  </Button>\n                </>\n              ) : (\n                <Button type=\"submit\" variant=\"primary\">\n                  Actualizar categora\n                </Button>\n              )}\n            </Form>\n          </FormContainer>\n        )}\n      </main>\n    </div>\n  );\n}\n","import {\n  USER_DELETE_FAIL,\n  USER_DELETE_REQUEST,\n  USER_DELETE_RESET,\n  USER_DELETE_SUCESS,\n  USER_DETAILS_FAIL,\n  USER_DETAILS_REQUEST,\n  USER_DETAILS_RESET,\n  USER_DETAILS_SUCESS,\n  USER_LIST_FAIL,\n  USER_LIST_REQUEST,\n  USER_LIST_RESET,\n  USER_LIST_SUCESS,\n  USER_LOGIN_FAIL,\n  USER_LOGIN_REQUEST,\n  USER_LOGIN_SUCESS